<h1>Lideres</h1>
<div class="row">
  <div class="col-md-10">
    <table class="table">
      <thead class="thead-light">
        <tr>
          <th scope="col">#</th>
          <th scope="col">DOCUMENTO</th>
          <th scope="col">NOMBRE</th>
          <th scope="col">CORREO</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user|%>
            <tr>
              <th scope="row"><%= link_to user.id , user_path(user.id)%></th>
              <td scope="row"><%= link_to user.cedula , user_path(user.id)%></td>
              <td scope="row"><%= link_to  user.nombre , user_path(user.id)%></td>
              <td scope="row"><%= link_to  user.email , user_path(user.id)%></td>
            </tr>
        <%end%>
      </tbody>
    </table>
  </div>
  <div class="col-md-2">
  </div>
</div>
